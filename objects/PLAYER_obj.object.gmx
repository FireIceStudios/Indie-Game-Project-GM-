        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// PLATFORM PHYSICS

var rkey = keyboard_check(vk_right);
var lkey = keyboard_check(vk_left);
var jkey = keyboard_check(ord("A"));


// CHECK FOR GROUND
if (place_meeting(x, y+1, ICE_GROUND1_obj)) {
    vspd = 0;


    // JUMPING
    if (jkey) {
        vspd = -jspd;
        }
        
} else {

    // GRAVITY
    if (vspd &lt; 10) {
        vspd += grav;
        }
}

// MOVING RIGHT
if (rkey) {
    hspd = spd;
}


// MOVING LEFT
if (lkey) {
    hspd = -spd;
}


// CHECK FOR NOT MOVING
if ((!rkey &amp;&amp; !lkey) || (rkey &amp;&amp; lkey)) {
    hspd = 0;
}




// HORIZONTAL COLLISIONS
if (place_meeting(x+hspd, y, ICE_GROUND1_obj)) {
    while(!place_meeting(x+sign(hspd), y, ICE_GROUND1_obj)) {
        x += sign(hspd);
    }
    hspd = 0;
}


// MOVE HORIZONTALLY
x += hspd;


// VERTICAL COLLISIONS
if (place_meeting(x, y+vspd, ICE_GROUND1_obj)) {
    while(!place_meeting(x, y+sign(vspd), ICE_GROUND1_obj)) {
        y += sign(vspd);
    }
    vspd = 0;
}



// MOVE VERTICALLY
y += vspd;
</string>
